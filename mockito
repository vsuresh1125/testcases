
package com.dfs.calleffdrvs;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

class CardAccountNumberLookupInputVOTest {

    @Test
    void testGetterAndSetter() {
        // Create an instance of the class
        CardAccountNumberLookupInputVO vo = new CardAccountNumberLookupInputVO();

        // Set values using setters
        vo.setAcctKey("12345");
        vo.setTrnsfrAcctSeqNbr("67890");

        // Verify values using getters
        assertEquals("12345", vo.getAcctKey(), "acctKey should be 12345");
        assertEquals("67890", vo.getTrnsfrAcctSeqNbr(), "trnsfrAcctSeqNbr should be 67890");
    }

    @Test
    void testDefaultValues() {
        // Create an instance of the class
        CardAccountNumberLookupInputVO vo = new CardAccountNumberLookupInputVO();

        // Verify default values are null
        assertEquals(null, vo.getAcctKey(), "acctKey should be null by default");
        assertEquals(null, vo.getTrnsfrAcctSeqNbr(), "trnsfrAcctSeqNbr should be null by default");
    }
}
